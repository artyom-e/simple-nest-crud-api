model Task {
  id          Int      @id @default(autoincrement())
  title       String   @db.VarChar(255)
  description String?
  isCompleted Boolean  @default(false) @map("is_completed")
  taskListId  Int      @map("task_list_id")
  taskList    TaskList @relation(fields: [taskListId], references: [id])

  @@map("tasks")
}
